{
  "name": "Forest",
  "type": "light",
  "colors": {
    "primary": {
      "50": "#f0fdf4",
      "100": "#dcfce7",
      "200": "#bbf7d0",
      "300": "#86efac",
      "400": "#4ade80",
      "500": "#22c55e",
      "600": "#16a34a",
      "700": "#15803d",
      "800": "#166534",
      "900": "#14532d",
      "950": "#052e16"
    },
    "secondary": {
      "50": "#fefce8",
      "100": "#fef9c3",
      "200": "#fef08a",
      "300": "#fde047",
      "400": "#facc15",
      "500": "#eab308",
      "600": "#ca8a04",
      "700": "#a16207",
      "800": "#854d0e",
      "900": "#713f12",
      "950": "#422006"
    },
    "accent": {
      "50": "#ecfdf5",
      "100": "#d1fae5",
      "200": "#a7f3d0",
      "300": "#6ee7b7",
      "400": "#34d399",
      "500": "#10b981",
      "600": "#059669",
      "700": "#047857",
      "800": "#065f46",
      "900": "#064e3b",
      "950": "#022c22"
    }
  },
  "components": {
    "background": {
      "primary": "bg-white",
      "secondary": "bg-green-50",
      "tertiary": "bg-emerald-50",
      "gradient": "bg-gradient-to-br from-green-50 to-emerald-100"
    },
    "text": {
      "primary": "text-slate-900",
      "secondary": "text-slate-700",
      "tertiary": "text-slate-600",
      "accent": "text-green-600",
      "muted": "text-slate-500"
    },
    "border": {
      "primary": "border-green-200",
      "secondary": "border-emerald-200",
      "accent": "border-green-300",
      "focus": "border-green-500"
    },
    "button": {
      "primary": {
        "base": "bg-green-600 text-white border-green-600",
        "hover": "hover:bg-green-700 hover:border-green-700",
        "active": "active:bg-green-800",
        "disabled": "disabled:bg-gray-300 disabled:text-gray-500"
      },
      "secondary": {
        "base": "bg-white text-green-700 border-green-300",
        "hover": "hover:bg-green-50 hover:border-green-400",
        "active": "active:bg-green-100",
        "disabled": "disabled:bg-gray-100 disabled:text-gray-400"
      },
      "ghost": {
        "base": "bg-transparent text-green-700",
        "hover": "hover:bg-green-100",
        "active": "active:bg-green-200"
      }
    },
    "card": {
      "base": "bg-white border-green-200 shadow-sm",
      "hover": "hover:shadow-lg hover:border-green-300",
      "focus": "focus:ring-2 focus:ring-green-500 focus:ring-offset-2"
    },
    "input": {
      "base": "bg-white border-green-300 text-slate-900 placeholder-slate-500",
      "focus": "focus:border-green-500 focus:ring-green-500",
      "error": "border-red-300 focus:border-red-500 focus:ring-red-500"
    },
    "sidebar": {
      "background": "bg-gradient-to-br from-green-600 via-emerald-600 to-teal-700",
      "text": "text-white",
      "accent": "bg-white/10",
      "hover": "hover:bg-white/20"
    },
    "gameBoard": {
      "cell": {
        "base": "bg-gradient-to-br from-white/90 to-green-50/90 border-green-200/50",
        "hover": "hover:bg-gradient-to-br hover:from-green-50/90 hover:to-emerald-50/90 hover:border-green-300/50",
        "winner": "bg-gradient-to-br from-emerald-100 to-emerald-200 border-emerald-400/70",
        "disabled": "opacity-60 cursor-not-allowed"
      },
      "symbol": {
        "x": "text-green-600",
        "o": "text-emerald-600"
      }
    }
  },
  "animations": {
    "transition": "transition-all duration-300 ease-in-out",
    "hover": "transform hover:scale-105",
    "pulse": "animate-pulse",
    "bounce": "animate-bounce",
    "fade": "animate-fade-in"
  }
}
