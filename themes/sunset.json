{
  "name": "Sunset",
  "type": "light",
  "colors": {
    "primary": {
      "50": "#fef2f2",
      "100": "#fee2e2",
      "200": "#fecaca",
      "300": "#fca5a5",
      "400": "#f87171",
      "500": "#ef4444",
      "600": "#dc2626",
      "700": "#b91c1c",
      "800": "#991b1b",
      "900": "#7f1d1d",
      "950": "#450a0a"
    },
    "secondary": {
      "50": "#fff7ed",
      "100": "#ffedd5",
      "200": "#fed7aa",
      "300": "#fdba74",
      "400": "#fb923c",
      "500": "#f97316",
      "600": "#ea580c",
      "700": "#c2410c",
      "800": "#9a3412",
      "900": "#7c2d12",
      "950": "#431407"
    },
    "accent": {
      "50": "#fefce8",
      "100": "#fef9c3",
      "200": "#fef08a",
      "300": "#fde047",
      "400": "#facc15",
      "500": "#eab308",
      "600": "#ca8a04",
      "700": "#a16207",
      "800": "#854d0e",
      "900": "#713f12",
      "950": "#422006"
    }
  },
  "components": {
    "background": {
      "primary": "bg-white",
      "secondary": "bg-orange-50",
      "tertiary": "bg-red-50",
      "gradient": "bg-gradient-to-br from-orange-50 to-red-100"
    },
    "text": {
      "primary": "text-slate-900",
      "secondary": "text-slate-700",
      "tertiary": "text-slate-600",
      "accent": "text-orange-600",
      "muted": "text-slate-500"
    },
    "border": {
      "primary": "border-orange-200",
      "secondary": "border-red-200",
      "accent": "border-orange-300",
      "focus": "border-orange-500"
    },
    "button": {
      "primary": {
        "base": "bg-orange-600 text-white border-orange-600",
        "hover": "hover:bg-orange-700 hover:border-orange-700",
        "active": "active:bg-orange-800",
        "disabled": "disabled:bg-gray-300 disabled:text-gray-500"
      },
      "secondary": {
        "base": "bg-white text-orange-700 border-orange-300",
        "hover": "hover:bg-orange-50 hover:border-orange-400",
        "active": "active:bg-orange-100",
        "disabled": "disabled:bg-gray-100 disabled:text-gray-400"
      },
      "ghost": {
        "base": "bg-transparent text-orange-700",
        "hover": "hover:bg-orange-100",
        "active": "active:bg-orange-200"
      }
    },
    "card": {
      "base": "bg-white border-orange-200 shadow-sm",
      "hover": "hover:shadow-lg hover:border-orange-300",
      "focus": "focus:ring-2 focus:ring-orange-500 focus:ring-offset-2"
    },
    "input": {
      "base": "bg-white border-orange-300 text-slate-900 placeholder-slate-500",
      "focus": "focus:border-orange-500 focus:ring-orange-500",
      "error": "border-red-300 focus:border-red-500 focus:ring-red-500"
    },
    "sidebar": {
      "background": "bg-gradient-to-br from-orange-600 via-red-600 to-pink-700",
      "text": "text-white",
      "accent": "bg-white/10",
      "hover": "hover:bg-white/20"
    },
    "gameBoard": {
      "cell": {
        "base": "bg-gradient-to-br from-white/90 to-orange-50/90 border-orange-200/50",
        "hover": "hover:bg-gradient-to-br hover:from-orange-50/90 hover:to-red-50/90 hover:border-orange-300/50",
        "winner": "bg-gradient-to-br from-yellow-100 to-yellow-200 border-yellow-400/70",
        "disabled": "opacity-60 cursor-not-allowed"
      },
      "symbol": {
        "x": "text-orange-600",
        "o": "text-red-600"
      }
    }
  },
  "animations": {
    "transition": "transition-all duration-300 ease-in-out",
    "hover": "transform hover:scale-105",
    "pulse": "animate-pulse",
    "bounce": "animate-bounce",
    "fade": "animate-fade-in"
  }
}
